#!/usr/bin/env python
# -*- coding: utf-8 -*-

# roslaunch2 modules:
from roslaunch2 import *


def main(**kwargs):
    root = Launch()
    pkg = Package('rostopic')

    # Define a ROS node to test (without namespace):
    t = Test(pkg, 'rostopic', 'name_of_this_test', args="""pub /foo/bar std_msgs/String "data: ''" """)
    t.retry = 5
    t.time_limit = 120.0
    root.add(t)
    return root
